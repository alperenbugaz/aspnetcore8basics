@model Ogretmen

@{
    ViewData["Title"] = "Create";
}
<form asp-controller="Ogretmen" asp-action="Create" method="post"> 
    <div class="mb-3">
        <label asp-for="Ad" class="form-label">Ad</label>
        <input asp-for="Ad" type="text" class="form-control">
        <span asp-validation-for="Ad"></span>
    </div>

    <div class="mb-3">
        <label asp-for="Soyad" class="form-label">Soyad</label>
        <input asp-for="Soyad" type="text" class="form-control">
        <span asp-validation-for="Soyad"></span>

    </div>

    <div class="mb-3">
        <label asp-for="Email" class="form-label">E-Mail</label>
        <input asp-for="Email" type="text" class="form-control">
        <span asp-validation-for="Email"></span>

    </div>

    <div class="mb-3">
        <label asp-for="Telefon" class="form-label">Telefon</label>
        <input asp-for="Telefon" type="text" class="form-control">
        <span asp-validation-for="Telefon"></span>
        
    </div>

    <div class="mb-3">
        <label asp-for="BaslamaTarihi" class="form-label">Ba≈ülama Tarihi</label>
        <input asp-for="BaslamaTarihi" class="form-control">
        <span asp-validation-for="BaslamaTarihi"></span>
        
    </div>
    <button type="submit" class="btn btn-secondary">Kaydet</button>
</form>